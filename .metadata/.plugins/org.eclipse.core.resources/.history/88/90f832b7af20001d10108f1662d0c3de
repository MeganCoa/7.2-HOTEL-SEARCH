package co.grandcircus72.HOTEL.SEARCH.models;

import org.springframework.data.annotation.Id;
import org.springframework.data.mongodb.core.mapping.Document;

@Document("hotels")
public class Hotels {
	
	@Id
	private String id;
	private String name;
	private String city;
	private Integer pricePerNight;
	
	
	
	
}
